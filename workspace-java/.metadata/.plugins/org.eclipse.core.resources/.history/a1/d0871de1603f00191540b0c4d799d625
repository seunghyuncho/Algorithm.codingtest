package tsys.codingtest;

import java.util.Scanner;

public class AscendingDecendingMixed1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int n = 0;
		int cnt = 0;
		int arr[] = new int[7];
		while (sc.hasNextInt()) {
			n = sc.nextInt();
			arr[cnt] = n;
			cnt++;
		}
		sc.close();
	}

}
